{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/cookie/random",
        "dest": "/server.js"
      },
      {
        "src": "/cookie/add",
        "dest": "/server.js"
      },
      {
        "src": "/cookie/:id",
        "dest": "/server.js"
      },
      {
        "src": "/cookie",
        "dest": "/server.js"
      },
      {
        "src": "/cookie/:id",
        "methods": ["DELETE"],
        "dest": "/server.js"
      },
      {
        "src": "/",
        "dest": "/server.js"
      }
    ]
  }
  